import Vue from 'vue'
import Router from 'vue-router'
import Index from '~/pages'
// import AdminIndex from '~/views/6tw9sQs/admin'

Vue.use(Router)

export function createRouter () {
    return new Router({
        mode: 'history',
        routes: [
            {
                path: '/',
                name: 'index',
                component: Index
            },
            {
                name: '6tw9sQs-admin',
                path: '/6tw9sQs/admin',
                component: () => import('~/pages/6tw9sQs/admin')
            },
        ]
    })
}
